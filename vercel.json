{
  "functions": {
    "src/app/**/*.{js,ts,tsx}": {
      "maxDuration": 60
    }
  },
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "redirects": [],
  "rewrites": [],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}